{
  "problemMatcher": [
    {
      "owner": "rust",
      "pattern": [
        {
          "regexp": "^(warning|warn|error)(\\[(.*)\\])?: (.*)$",
          "severity": 1,
          "message": 4,
          "code": 3
        },
        {
          "regexp": "^([\\s->=]*(.*):(\\d*):(\\d*)|.*)$",
          "file": 2,
          "line": 3,
          "column": 4
        }
      ]
    }
  ]
}
